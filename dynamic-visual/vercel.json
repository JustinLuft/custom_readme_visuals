{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/linkedin", "dest": "/api/linkedin.js" },
    { "src": "/api/personalwebsite", "dest": "/api/personalwebsite.js" },
    { "src": "/api/header", "dest": "/api/header.js" },
    { "src": "/api/languages", "dest": "/api/languages.js" },
    { "src": "/api/line1", "dest": "/api/line1.js" }
  ]
}
